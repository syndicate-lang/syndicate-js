  "dependencies": {
    "@syndicate-lang/core": "file:../../../core"
  },
  "devDependencies": {
    "@syndicate-lang/syndicatec": "file:../..",
    "rollup": "^2.37.0",
    "rollup-plugin-sourcemaps": "^0.6.3"
  }
}
