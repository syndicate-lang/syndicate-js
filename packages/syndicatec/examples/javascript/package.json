{
  "name": "@syndicate-lang/syndicatec-javascript-example",
  "version": "0.0.0",
  "description": "Simple syndicatec example in JavaScript",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run compile && npm run rollup",
    "compile": "npx syndicatec -d lib -b src 'src/**/*.js'",
